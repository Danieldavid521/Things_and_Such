mydb <- dbConnect(RSQLite::SQLite(), "")
dbWriteTable(mydb, "mtcars", mtcars)
mydb_query <- dbGetQuery(mydb, 'SELECT * FROM mtcars LIMIT 5')
# Define UI for application that draws a histogram
ui <- fluidPage(
  
